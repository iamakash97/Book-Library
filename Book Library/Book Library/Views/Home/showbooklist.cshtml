@model IEnumerable<BOL.Book>
@{
    ViewBag.Title = "showbooklist";
}

    
        <h1>Hey Welcome !!</h1>
        <center>
            <h2>List of Books</h2>
        </center>
            <div class="container">
                <table class="table table-striped">
                    <thead>
                        <tr>
                            <th>Sr No</th>
                            <th>Title</th>
                            <th>Author</th>
                            <th>ISBN</th>
                            <th>Publisher</th>
                            <th>Year</th>

                        </tr>
                    </thead>
                    <tbody>
                        @{

                            foreach (BOL.Book b in Model)
                            {
                                <tr>
                                    <td>@b.serno</td>
                                    <td>@b.title</td>
                                    <td>@b.author</td>
                                    <td>@b.ISBN</td>
                                    <td>@b.publisher</td>
                                    <td>@b.year</td>
                                </tr>
                            }
                        }
                    </tbody>


                </table>
            </div>
